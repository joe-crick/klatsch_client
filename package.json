    "sass-loader": "^3.2.0",
    "slugger": "^1.0.0",
    "socket.io": "^1.7.2",
    "standard": "^4.3.1",
    "style-loader": "^0.12.3",
    "surge": "^0.14.2",
    "url-loader": "^0.5.6",
    "webpack": "^2.2.1",
    "xhr": "^2.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "autoprefixer-core": "^5.2.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
