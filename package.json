{
  "name": "klatch",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "system": {
    "transpiler": "babel",
    "babelOptions": {
      "blacklist": []
    }
  },
  "scripts": {
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "node_modules/.bin/rimraf ./public",
    "build:index": "babel-node build/build-index.js",
    "build:img": ". build/./copy-images.sh",
    "build:dev": "webpack -d",
    "build:pre": "npm-run-all clean-public lint build:index build:img",
    "build:pre:temp": "npm-run-all clean-public build:index build:img",
    "build:prod": "babel-node build/build.js",
    "build": "npm-run-all build:pre:temp build:prod build:index",
    "webpack:server": "webpack-dev-server --inline",
    "webpack:debug": "webpack --display-error-details",
    "dev": "npm-run-all build:dev webpack:server",
    "lint": "eslint --ext .jsx,.js src/**",
    "tape-tests": "babel-tape-runner tests/**/*.js | faucet",
    "test": "npm-run-all tape-tests"
  },
  "dependencies": {
    "auth0-lock": "^10.11.0",
    "axios": "^0.15.3",
    "data.either": "^1.5.0",
    "data.maybe": "^1.2.2",
    "feathers-client": "^1.9.0",
    "immutable": "^3.8.1",
    "jumpsuit": "^2.1.1",
    "local-links": "^1.4.0",
    "lodash.assign": "^3.2.0",
    "lodash.has": "^3.2.1",
    "lodash.merge": "^3.3.1",
    "lodash.pick": "^3.1.0",
    "lodash.result": "^3.1.2",
    "milliseconds": "^1.0.3",
    "node-libs-browser": "^0.5.2",
    "octicons": "^2.2.0",
    "qs": "^3.1.0",
    "ramda": "^0.23.0",
    "ramda-fantasy": "^0.7.0",
    "react": "^15.2.0",
    "react-datepicker": "^0.28.1",
    "react-dom": "^15.1.0",
    "react-draggable": "^2.2.3",
    "react-dropzone": "^3.10.0",
    "react-hot-loader": "^1.2.7",
    "react-redirect": "^1.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-form": "^6.5.0",
