    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "css-loader": "^0.15.6",
    "del": "^2.2.2",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
